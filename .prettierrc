{
  "tabWidth": 2,
  "printWidth": 120,
  "bracketSameLine": true,
  "singleQuote": true,
  "plugins": ["prettier-plugin-toml"],
  "overrides": [
    {
      "files": ["*.scss", "*.css"],
      "options": {
        "tabWidth": 4
      }
    },
    {
      "files": ["*.ts", "*.js", "*.mjs"],
      "options": {
        "tabWidth": 4
      }
    }
  ]
}
